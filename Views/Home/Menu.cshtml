@{
    Layout = null;
}


@foreach (var item in ViewBag.menu)
{
    <li class="nav-item">
        <a class="dropdown-item py-2 text-dark" asp-route-cate="@item" asp-controller="Product" asp-action="Category">@item</a>
    </li>
}

